{
  "name": "angular-miniprogram",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:ci": "npm run build:library && npm run test:jasmine library&& npm run test:jasmine",
    "test": "npm run test:jasmine",
    "test:jasmine": "tsc -p ./script/tsconfig.startup-jasmine.json && node ./script/startup-jasmine.js",
    "prebuild": "rimraf ./dist",
    "build": " npm run build:library && npm run build:builder && npm run copy:assets && npm run build:karma",
    "copy:assets": "cpx \"./src/builder/**/*.json\" ./dist/builder && cpx \"./src/builder/**/*.js\" ./dist/builder && cpx ./readme.md ./dist",
    "hook-code": "npm run forms-hook && npm run common-hook",
    "build:library": "node ./script/start-build-library",
    "build:builder": "ts-node -P ./script/tsconfig.json ./script/build.ts ./tsconfig.builder.json",
    "build:karma": "ts-node -P ./script/tsconfig.json ./script/build.ts ./src/builder/karma/client/tsconfig.json && ts-node -P ./script/tsconfig.json ./script/build.ts ./src/builder/karma/plugin/tsconfig.json",
    "prepare": "husky install",
    "lint": "eslint --max-warnings 0 \"./**/*.ts\"",
    "forms-hook": "ts-node ./script/run-forms-hook.ts",
    "common-hook": "ts-node ./script/run-common-hook.ts",
    "coverage": "nyc npm run test",
    "typedoc": "typedoc",
    "deploy": "npm run typedoc && npm run build:library && npm run test:jasmine library && npm run coverage-badge -- init && cpx \"./deploy/doc/**/*\" ./docs && cpx \"./deploy/api-doc/**/*\" ./docs/api-doc && npm run coverage && npm run coverage-badge -- success && cpx ./deploy/doc/assets/img/badge.svg ./docs/assets/img",
    "coverage-badge": "ts-node ./script/coverage-badge.ts"
  },
  "private": true,
  "author": "wszgrcy",
  "license": "MIT",
  "devDependencies": {
    "@angular-devkit/architect": "0.1600.3",
    "@angular-devkit/build-angular": "16.0.3",
    "@angular-devkit/core": "16.0.3",
    "@angular-devkit/schematics": "16.0.3",
    "@angular/compiler": "16.0.3",
    "@angular/compiler-cli": "16.0.3",
    "@angular/core": "16.0.3",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@ngtools/webpack": "16.0.3",
    "@types/fs-extra": "^9.0.12",
    "@types/glob": "^7.2.0",
    "@types/jasmine": "^3.7.7",
    "@types/karma": "^6.3.3",
    "@types/node": "16.11.68",
    "@types/socket.io-client": "^3.0.0",
    "@types/webpack-sources": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "cyia-ngx-devkit": "0.0.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "fs-extra": "^10.1.0",
    "glob": "^7.2.0",
    "husky": "^7.0.0",
    "jasmine": "3.10.0",
    "jasmine-core": "4.0.1",
    "karma": "^6.3.17",
    "mini-types": "^0.1.7",
    "miniprogram-api-typings": "^3.6.0",
    "ng-packagr": "16.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "static-injector": "3.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typedoc": "^0.24.7",
    "typescript": "~5.0.2",
    "weapp.socket.io": "^3.0.0",
    "webpack": "5.80.0",
    "zone.js": "~0.13.0"
  },
  "dependencies": {
    "cyia-code-util": "^1.8.0",
    "nyc": "^15.1.0",
    "webpack-bootstrap-assets-plugin": "^2.0.3"
  },
  "resolutions": {
    "ng-packagr/rxjs": "^7.4.0",
    "inquirer/rxjs": "^7.4.0"
  }
}
